{
  "name": "soamn/maple",
  "description": "Beautiful Tailwind and Alpine components for Laravel Blade",
  "autoload": {
    "psr-4": {
      "Soamn\\Maple\\": "src/"
    }
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "soamn",
      "email": "negiaman604@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
    "gehrisandro/tailwind-merge-laravel": "^1.3"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Soamn\\Maple\\UiServiceProvider"
      ]
    }
  },
  "archive": {
    "exclude": [
      "/assets",
      "/readme.md"
    ]
  },
  "minimum-stability": "1.0.0",
  "prefer-stable": true
}
